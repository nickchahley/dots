#!/usr/bin/env python3
""" 
Print versions of all packages in args. Works for pkgs installed with git+ssh where `pip freeze` does not
"""
import sys
from importlib.metadata import version

packages = sys.argv[1:]
for pkg in packages:
    try:
        print(f"{pkg} = {version(pkg)}")
    except ModuleNotFoundError:
        print(f"{pkg} not found")

