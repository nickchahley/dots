#!/bin/bash
# After compiling a program (mako is the instigator) man pages might be found in
# in the form of <name>.<n>.scd
# /usr/share/man/man1 or /usr/local/share/man/man<n>
sudo cp -v *.1.scd /usr/local/share/man/man1 
sudo cp -v *.3.scd /usr/local/share/man/man3 
sudo cp -v *.5.scd /usr/local/share/man/man5
