# A profile will be enabled if all listed outputs are connected
# swaymsg -t get_outputs -p | grep Output
# Use " instead of '

profile imac {
	# tput DP-1 'Apple Computer Inc Color LCD 0x00000000'
	output "eDP-1" position 0,0
	output "Apple Computer Inc Color LCD 0x00000000" position 1920,0 mode 2560x1440
	exec bash ~/.config/kanshi/set-workspace
}

profile yodoit-h {
	output "eDP-1" position 0,0
	output "Unknown TYPEC demoset-1" position 1920,0
	exec bash ~/.config/kanshi/set-workspace
}

profile work-three {
	output "eDP-1" position 0,0
	output  "Hewlett Packard HP LA1956x 3CQ2342KMV" position 1920,0 mode 1280x1024
	output "Dell Inc. DELL P2419H JZFYDZ2" position 3200,-400 transform 270

	exec swaymsg workspace 1 output "eDP-1"
	exec swaymsg workspace 2 output "eDP-1"
	exec swaymsg workspace 3 output "eDP-1"
	exec swaymsg workspace 4 output "eDP-1"
	exec swaymsg workspace 5 output "eDP-1"

	exec swaymsg workspace 6 output  "DP-1"
	exec swaymsg workspace 7 output  "DP-1"
	exec swaymsg workspace 8 output  "DP-1"
	exec swaymsg workspace 9 output  "DP-1"
	exec swaymsg workspace 10 output "DP-1"
	exec swaymsg workspace 11 output "DP-1"
	exec swaymsg workspace 12 output "DP-1"
	exec swaymsg workspace 13 output "DP-1"

	exec swaymsg workspace 14 output "HDMI-1"
	exec swaymsg workspace 15 output "HDMI-1"
	exec swaymsg workspace 16 output "HDMI-1"
	exec swaymsg workspace 17 output "HDMI-1"

	exec swaymsg workspace 1
	exec swaymsg workspace 14
	exec swaymsg workspace 6

}

profile work-one {
	output "eDP-1" position 0,0
	# output "Acer Technologies ED320QR S TJNAA0033W01" position 1920,0 mode 1920x1080
	output  "Hewlett Packard HP LA1956x 3CQ2342KMV" position 1920,0 mode 1280x1024

	exec swaymsg workspace 1 output "eDP-1"
	exec swaymsg workspace 2 output "eDP-1"
	exec swaymsg workspace 3 output "eDP-1"
	exec swaymsg workspace 4 output "eDP-1"
	exec swaymsg workspace 5 output "eDP-1"

	exec swaymsg workspace 6 output  "DP-1"
	exec swaymsg workspace 7 output  "DP-1"
	exec swaymsg workspace 8 output  "DP-1"
	exec swaymsg workspace 9 output  "DP-1"
	exec swaymsg workspace 10 output "DP-1"
	exec swaymsg workspace 11 output "DP-1"
	exec swaymsg workspace 12 output "DP-1"
	exec swaymsg workspace 13 output "DP-1"

	exec swaymsg workspace 1
	exec swaymsg workspace 6
}

profile home-hdmi {
	output "eDP-1" position 0,1200
	output "HDMI-A-1" position 0,0

	exec swaymsg workspace 1 output "eDP-1"
	exec swaymsg workspace 2 output "eDP-1"
	exec swaymsg workspace 3 output "eDP-1"
	exec swaymsg workspace 4 output "eDP-1"
	exec swaymsg workspace 5 output "eDP-1"

	exec swaymsg workspace 6 output  "HDMI-A-1"
	exec swaymsg workspace 7 output  "HDMI-A-1"
	exec swaymsg workspace 8 output  "HDMI-A-1"
	exec swaymsg workspace 9 output  "HDMI-A-1"
	exec swaymsg workspace 10 output "HDMI-A-1"
}

profile home-one { 
	output "eDP-1" position 0,1200
	output "Dell Inc. DELL U2415 CFV9N6B21Y5S" position 0,0 mode 1920x1200

	exec swaymsg workspace 1 output "eDP-1"
	exec swaymsg workspace 2 output "eDP-1"
	exec swaymsg workspace 3 output "eDP-1"
	exec swaymsg workspace 4 output "eDP-1"
	exec swaymsg workspace 5 output "eDP-1"

	exec swaymsg workspace 6 output  "DP-5"
	exec swaymsg workspace 7 output  "DP-5"
	exec swaymsg workspace 8 output  "DP-5"
	exec swaymsg workspace 9 output  "DP-5"
	exec swaymsg workspace 10 output "DP-5"
}

