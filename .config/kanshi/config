profile work-four {
	# This seems to require manual HDMI-A-1 transform 0 afterwards??
	output "eDP-1" position 0,0
	# output "Lenovo Group Limited L24q-20 U4P06BRV" position 1920,0
	output "HDMI-A-1" position 1920,0
	output  "Hewlett Packard HP LA1956x 3CQ2342KMV" position 4480,0 mode 1280x1024

	exec swaymsg workspace 1 output "eDP-1"
	exec swaymsg workspace 2 output "eDP-1"
	exec swaymsg workspace 3 output "eDP-1"
	exec swaymsg workspace 4 output "eDP-1"
	exec swaymsg workspace 5 output "eDP-1"

	exec swaymsg workspace 6 output  HDMI-A-1
	exec swaymsg workspace 7 output  HDMI-A-1
	exec swaymsg workspace 8 output  HDMI-A-1
	exec swaymsg workspace 9 output  HDMI-A-1
	exec swaymsg workspace 10 output HDMI-A-1
	exec swaymsg workspace 11 output HDMI-A-1
	exec swaymsg workspace 12 output HDMI-A-1

	exec swaymsg workspace 13 output DP-1
	exec swaymsg workspace 14 output DP-1
	exec swaymsg workspace 15 output DP-1
	exec swaymsg workspace 16 output DP-1
	exec swaymsg workspace 17 output DP-1
	exec swaymsg workspace 18 output DP-1
	exec swaymsg workspace 19 output DP-1

	exec swaymsg workspace 1
	exec swaymsg workspace 14
	exec swaymsg workspace 6
	
	exec swaymsg output "HDMI-A-1" transform 0

}

